@misc{RefWorks:doc:5e9bc906e4b037d586c749b5,
	author = 	 {Klaus Leopold and Siegfried Kaltenecker},
	year = 	 {2015},
	title = 	 {Corporate Culture and Politics},
	pages = 	 {133-141},
	note = 	 {ID: wilbooks10.1002/9781119019732.ch12},
	abstract = 	 {Summary Corporate culture is far more than a loose collection of cultural artifacts. It unites the cornerstones of corporate mission, organizational structures, and personal conduct. Culture encompasses every aspect of how things work in an organization. Similar to the iceberg model, organizational culture has three levels: artifacts, espoused values, and basic assumptions. While the first two levels are visible and audible, the basic assumptions remain mostly unspoken and often unconscious too. The deciding factor for the success of change projects is whether these cultural routes are functional or not, whether they help process the desired change productively, or do they rather block the change? Many improvement initiatives fail because the power of corporate culture is not acknowledged at all, or at least not acknowledged enough. Ignoring micropolitical behavior and the dysfunctionality of power games in change processes contributes to this problem.},
	isbn = 	 {9781-119019701},
	doi={10.1002/9781119019732.ch12}
}
